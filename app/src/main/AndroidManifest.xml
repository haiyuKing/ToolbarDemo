<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.why.project.toolbardemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".ToolBarNoMenuActivity"/>
        <activity android:name=".ToolBarTitleLeftActivity"/>
        <activity android:name=".ToolBarTitleCenterActivity"/>
        <activity android:name=".ToolBarNoNavigationiconActivity"/>
        <activity android:name=".ToolBarMenuTextActivity"/>
        <activity android:name=".ToolBarPopupMenuActivity"/>
        <activity android:name=".ToolBarDialogActivity"/>
        <activity android:name=".ToolBarMenuUpdateActivity"/>
    </application>

</manifest>